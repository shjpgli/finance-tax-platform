<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.abc12366.uc.mapper.db2.UpointsLotteryRoMapper">
    <select id="selectList" resultType="com.abc12366.uc.model.bo.UpointsLotteryBO" parameterType="java.util.Map">
        SELECT id,orderId,name,description,stock,image,luck,notluck,createTime,lastUpdate
        from uc_upoints_lottery
        <where>
            <if test="name!=null">
                name=#{name}
            </if>
        </where>
        ORDER BY orderId
    </select>
    <select id="selectOne" resultType="com.abc12366.uc.model.bo.UpointsLotteryBO" parameterType="java.lang.String">
SELECT  id,orderId,name,description,stock,image,luck,notluck,createTime,lastUpdate
 from uc_upoints_lottery 
where id=#{id}
</select>

</mapper>
