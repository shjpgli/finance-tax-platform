<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.abc12366.uc.mapper.db2.TagRoMapper">

    <select id="selectList" resultType="com.abc12366.uc.model.bo.TagBO" parameterType="com.abc12366.uc.model.bo.TagSelectParamBO">
        SELECT  id,tagName,status,createTime,lastUpdate,category,description,rule
        from uc_tag
        <where>
            <if test="tagName!=mull">
                tagName=#{tagName}
            </if>
            <if test="category!=mull">
                and category=#{category}
            </if>
        </where>
        ORDER BY lastUpdate DESC
    </select>

    <select id="selectOne" resultType="com.abc12366.uc.model.bo.TagBO" parameterType="java.lang.String">
        SELECT  id,tagName,status,createTime,lastUpdate,category,description,rule
        from uc_tag
        where id=#{id}
    </select>

</mapper>