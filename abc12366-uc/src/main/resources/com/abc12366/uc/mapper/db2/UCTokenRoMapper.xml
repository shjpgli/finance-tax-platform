<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.abc12366.uc.mapper.db2.UCTokenRoMapper">
    <select id="selectOne" resultType="UCToken">
        select id, appId, userId, token, lastTokenResetDate
        from uc_token
        where userId = #{userId}
        and appId = #{appId}
    </select>

</mapper>