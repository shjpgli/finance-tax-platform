        <association property="orderProductspecBOList" column="{productId=productId,orderNo=orderNo}"
                     select="getOrderProductspecBO"></association>
    </resultMap>

    <!--订单日志对象-->
    <resultMap type="com.abc12366.uc.model.bo.OrderLogBO" id="orderLogBOMap">
        <id column="id" property="id" jdbcType="VARCHAR"/>
