<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.abc12366.uc.mapper.db2.OrderBackRoMapper">

	<sql id="Base_Column_List">
		 id,	userId,	orderNo,	reason,	remark,	expressNo,	expressComp,	status,	createTime,	lastUpdate
	</sql>

	<select id="selectByPrimaryKey" resultType="com.abc12366.uc.model.OrderBack" parameterType="java.lang.String">
		 SELECT
		 <include refid="Base_Column_List" />
		 FROM uc_order_back
		 WHERE id = #{id}
	</select>

</mapper>