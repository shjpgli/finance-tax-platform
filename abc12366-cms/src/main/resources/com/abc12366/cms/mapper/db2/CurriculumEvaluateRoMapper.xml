<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.abc12366.cms.mapper.db2.CurriculumEvaluateRoMapper">

	<!-- 通用查询结果列-->
	<sql id="Base_Column_List">
		 evaluateId,	curriculumId,	grade,	studyFeel,	userId,	evaluateTime
	</sql>

	<!-- 查询（根据主键ID查询） -->
	<select id="selectByPrimaryKey" resultType="com.abc12366.cms.model.curriculum.CurriculumEvaluate" parameterType="java.lang.String">
		 SELECT
		 <include refid="Base_Column_List" />
		 FROM cms_curriculum_evaluate
		 WHERE evaluateId = #{evaluateId}
	</select>

    <!-- 查询（根据主键ID查询） -->
    <select id="selectList" resultType="com.abc12366.cms.model.curriculum.bo.CurriculumEvaluateBo" parameterType="java.util.Map">
        SELECT
        <include refid="Base_Column_List" />
        FROM cms_curriculum_evaluate
        WHERE 1=1
    </select>



</mapper>