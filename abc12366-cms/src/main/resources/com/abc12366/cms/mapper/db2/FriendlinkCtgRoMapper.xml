<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.abc12366.cms.mapper.db2.FriendlinkCtgRoMapper">

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        friendlinkctgId, siteId, friendlinkctgName, priority
    </sql>

    <!-- 查询（根据主键ID查询） -->
    <select id="selectByPrimaryKey" resultType="com.abc12366.cms.model.FriendlinkCtg" parameterType="java.lang.String">
        SELECT
        <include refid="Base_Column_List"/>
        FROM cms_friendlink_ctg
        WHERE friendlinkctgId = #{friendlinkctgId}
    </select>


</mapper>